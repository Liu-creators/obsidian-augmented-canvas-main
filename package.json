{
	"name": "obsidian-augmented-canvas",
	"version": "0.1.20",
	"description": "Obsidian Canvas with AI features powered by DeepSeek.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "jsdom",
		"testMatch": [
			"**/__tests__/**/*.test.ts"
		],
		"moduleNameMapper": {
			"^obsidian$": "<rootDir>/src/__mocks__/obsidian.ts"
		}
	},
	"keywords": [
		"obsidian",
		"canvas",
		"ai",
		"deepseek"
	],
	"author": "LiuZhuangZhuang",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^20.10.0",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.9",
		"eslint": "^8.56.0",
		"fast-check": "^4.5.3",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"obsidian": "latest",
		"ts-jest": "^29.4.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"fuse.js": "^7.0.0",
		"js-tiktoken": "^1.0.8",
		"monkey-around": "^2.3.0",
		"openai": "^4.25.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3"
	}
}
